@model Cuzdana

<form asp-controller="Cuzdan" asp-action="Paracek" method="post">
    <h1>
        Lütfen çekmek istediğiniz miktarı girin.
    </h1>

    <div class="form-group">
        <label for="cekilen">Çekilecek miktar:</label>
        <input type="number" id="cekilen" name="cekilen" class="form-control" />
    </div>
    @if (ViewData["hatamesaj"] != null)
    {
        <div class="text-danger">@ViewData["hatamesaj"]</div>
    }

    
    <h1>
    </h1>
    <button class="btn btn-lg btn-primary" type="submit">Çekimi Onayla</button>


</form>